<% provide(:title, 'All users') %>
<h1>All users</h1>
	<ul class="users">
		<% @users.each do |user| %>
			<li>
				<strong>name:</strong>
				<%= user.name %>
				<strong>email:</strong>
				<%= user.email %>
				<td><%= link_to 'Show', user_path(user)%></td>
			</li>
		<% end %>
	<%= paginate @users %>
	</ul>

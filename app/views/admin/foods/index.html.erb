<% provide(:title, 'All food') %>
  <% @foods.each do |food| %>
    <tr>
      <td><%= food.name %></td>
      <td><%= food.food_content %></td>
      <td><%= image_tag food.image if food.image.attached?%></td>
      <td><%= link_to 'Show', admin_food_path(food) %></td>
      <%# <td><%= link_to 'Edit', edit_food_path(food) %></td>
    | <%= button_to "delete", admin_food_path(food) , method: :delete, data: { confirm: "You sure?" } %>
    </tr>
    <br>
<% end %>
